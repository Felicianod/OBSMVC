@model IEnumerable<OBSMVC.Models.OBS_QUESTION>

@{
    ViewBag.Title = "Question Maintenance";
}
<br/>
<h2>Question Maintenance</h2>

@*<p>
        @Html.ActionLink("Create New", "Create")
    </p>*@
<table class="table">
    <tr>        
        <th>
            @Html.DisplayNameFor(model => model.obs_question_full_text)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.obs_question_short_text)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.obs_question_desc)
